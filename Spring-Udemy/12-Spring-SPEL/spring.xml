<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="robot" class="com.pshingavi.spring.Robot">
		<!-- <property name="speach" value="Hello from spring.xml"></property> -->
		<!-- <property name="speach" value="#{'Hello'}"></property> -->
		<property name="speach" value="#{randomSpeach.generateRandomSpeach()}"></property>
	</bean>
	
	<bean id="randomSpeach" class="com.pshingavi.spring.RandomSpeach"></bean>
</beans>
